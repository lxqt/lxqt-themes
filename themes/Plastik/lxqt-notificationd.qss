Notification {
    color: #fafafa;
    border: 1px solid #8d8d8d;
    border-radius: 2px;
    background: #fafafa;
    margin: 0px;
}

QLabel {
    color: #000000;
}

#closeButton {
    border: none;
    qproperty-icon: url(lxqt-notificationd/cancel.svg);
}

#closeButton:hover {
    border: 1px solid #8d8d8d;
    border-radius: 2px;
    background: #ffffff;
    qproperty-icon: url(lxqt-notificationd/cancel.svg);
}

#appLabel {
    font-weight: bold;
}
