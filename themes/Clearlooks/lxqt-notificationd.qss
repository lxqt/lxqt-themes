Notification {
    color: #1a1a1a;
    border: 1px solid #1a1a1a;
    border-radius: 3px;
    background: #f0f0f0;
    margin: 0px;
}

QLabel {
    color: #1a1a1a;
}

#closeButton {
    border: none;
    qproperty-icon: url(lxqt-notificationd/cancel.svg);
}

#closeButton:hover {
    border: 1px solid #a0a0a0;
    border-radius: 3px;
    background: #ffffff;
    qproperty-icon: url(lxqt-notificationd/cancel.svg);
}

#appLabel {
    font-weight: bold;
}
