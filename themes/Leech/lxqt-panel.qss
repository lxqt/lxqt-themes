/*
"Leech" LXQt theme
By Azuma Hazuki (wandering hazuki at gmail.com)
Refactored 25 Feb. 2023
*/

LXQtPanel, LXQtPanel #BackgroundWidget {
        color:                          #e0e0e0;
        background:                     #000000;
}

/* Panel Borders by position */
LXQtPanel[position="Bottom"] #BackgroundWidget { border-top: 1px solid #000000; }
LXQtPanel[position="Left"] #BackgroundWidget { border-right: 1px solid #000000; }
LXQtPanel[position="Right"] #BackgroundWidget { border-left: 1px solid #000000; }

/* Catch-all settings for panel plugins and their subwidgets 
   not otherwise styled in this theme: 
   #ColorPicker
   #Custom and #CustomButton
   #DirectoryMenu
   #Dom
   #LXQtMount
   #NetworkMonitor
   #Sensors
   #ShowDesktop
   #Spacer
*/      

Plugin, Plugin > QToolButton, Plugin > QWidget > QToolButton,
Plugin > QWidget > QToolButton > QWidget > QToolButton {
        color:                          #e0e0e0;
        background:                     #000000;

        padding:                        0px;
        margin:				0px;

        /* prevents unexpected borders/boxes on hover */
        border:                         0px solid transparent;
}

QToolTip {
	color:				#e0e0e0;
	background:			#1c2d1c;

	border:				1px solid #1c2d1c;
}

#TaskBar,
#DesktopSwitch,
#WorldClock,
#WorldClockContent {
	color:				#e0e0e0;
	background:			transparent;
}

#QuickLaunch,
#QuickLaunchPlaceHolder {
	margin-left:			2px;
	margin-right:			2px;
}

#StatusNotifierPlugin,
#StatusNotifierPlugin > QToolButton,
#StatusNotifierPlugin StatusNotifierButton {
	padding:			2px;
	margin-left:			1px;
}

#MainMenu {
	color:				#e0e0e0;
	background:			#000000;

	qproperty-icon:			url(mainmenu.svg);

	border:				4px solid transparent;
}

#MainMenu:hover {
	color:				#e0e0e0;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(hover.png) 4;
}

#MainMenu:pressed {
	color:				#e0e0e0;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(active.png) 4;
}

#MainMenu QLineEdit {
	color:				#e0e0e0;
	background:			#000000;

	selection-color:		#ffffff;
	selection-background-color:	#16162f;

	border:				2px solid transparent;
	border-image:			url(entry.png) 2;

	margin:				2px;
	padding:			2px;

	font:				normal;
}

QMenu, 
#MainMenu QMenu,
#MainMenu ActionView {
	color:				#e0e0e0;
	background:			#000000;

	border:				1px solid #2f2f2f;

	min-width:			100px;

	font:				normal;
}

QMenu QToolButton {
	color:				#e0e0e0;
	background:			#000000;

	font:				bold;

	border:				4px solid transparent;
	border-image:			url(button.png) 4;

	padding:			2px;
}

QMenu::icon, 
#MainMenu QMenu::icon, 
#MainMenu ActionView::icon {
	padding-left:			7px;
}

QMenu::indicator, 
#MainMenu QMenu::indicator {
	padding-left:			5px;
}

QMenu::indicator:non-exclusive { image: url(unchecked.png);}
QMenu::indicator:non-exclusive:checked { image: url(checked.png);}
QMenu::indicator:non-exclusive:checked:selected { image: url(checked.png);}
QMenu::indicator:non-exclusive:unchecked { image: url(unchecked.png);}
QMenu::indicator:non-exclusive:unchecked:selected { image: url(unchecked.png);}

QMenu::item, 
#MainMenu QMenu::item,
#MainMenu ActionView::item {
	border:				4px solid transparent;

	padding-left:			6px;
	padding-right:			12px;
	padding-top:			0px;
	padding-bottom:			0px;

	color:				#e0e0e0;
	background:			transparent;

	min-width:			100px;

	margin:				1px;
}

QMenu::item:selected, 
#MainMenu QMenu::item:selected, 
#MainMenu ActionView::item:hover,
#MainMenu ActionView::item:selected {
	color:				#ffffff;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(hover.png) 4;
}

QMenu::item:disabled, 
#MainMenu QMenu::item:disabled,
#MainMenu ActionView::item:disabled {
	color:				#52501b;
	background:			transparent;
}

#TaskBar QToolButton,
#DesktopSwitch QToolButton {
	color:				#e0e0e0;
	background:			#000000;

	border:				4px solid transparent;
	border-image:			url(button.png) 4;

	padding-left:			1px;
	padding-right:			1px;
}

#TaskBar QToolButton:on,
#DesktopSwitch QToolButton:on {
	color:				#e0e0e0;
	background-color:		#16162f;

	border:				4px solid transparent;
	border-image:			url(active.png) 4;
}

#TaskBar QToolButton:hover,
#DesktopSwitch QToolButton:hover {
	color:				#e0e0e0;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(hover.png) 4;
}

#TaskBar QToolButton:pressed,
#DesktopSwitch QToolButton:pressed {
	color:				#e0e0e0;
	background-color:		#16162f;

	border:				4px solid transparent;
	border-image:			url(active.png) 4;
}

#TaskBar QToolButton:on:hover,
#DesktopSwitch QToolButton:on:hover {
	color:				#e0e0e0;
	background-color:		#16162f;

	border:				4px solid transparent;
	border-image:			url(hover-on.png) 4;
}

#TaskBar QToolButton[urgent="true"],
#TaskBar QToolButton[urgent="true"]:hover,
#DesktopSwitch QToolButton[urgent="true"],
#DesktopSwitch QToolButton[urgent="true"]:hover {
	color:				#e0e0e0;
	background:			#555555;

	border:				4px solid transparent;
	border-image:			url(urgent.png) 4;
}

#QuickLaunch QToolButton {
	color:				#e0e0e0;
	background:			#000000;

	border:				4px solid transparent;

	padding:			0px;
}

#Dom,
#ColorPicker,
#ShowDesktop {
	border:				4px solid transparent;
}

#Dom:hover,
#ColorPicker:hover,
#ColorPickerColorButton:hover,
#ColorPickerPickerButton:hover,
#ShowDesktop:hover,
#QuickLaunch QToolButton:hover {
	background:			#16162f;
	border-image:			url(button.png) 4;
}

#Dom:pressed,
#ColorPicker:pressed,
#ColorPickerColorButton:pressed,
#ColorPickerPickerButton:pressed,
#ShowDesktop:pressed,
#QuickLaunch QToolButton:pressed {
	background:			#16162f;
	border-image:			url(active.png) 4;
}

#WingMenuPlugin {
       	color:                          #e0e0e0;
       	background:                     #000000;

       	qproperty-icon:                 url(mainmenu.svg);

       	border:                         4px solid transparent;
}

#WingMenuPlugin:hover {
       	color:                          #e0e0e0;
       	background:                     #16162f;

       	border:                         4px solid transparent;
       	border-image:                   url(hover.png) 4;
}

#WingMenuPlugin:pressed {
       	color:                          #e0e0e0;
       	background:                     #16162f;

       	border:                         4px solid transparent;
       	border-image:                   url(active.png) 4;
}

#WingMenuPlugin QWidget {
	/* "Zero out" Wing Menu widgets */
	color:				#e0e0e0;
	background:			#000000;
}

#WingMenuPlugin QMenu {
	background:			#000000;

	border:				1px solid #2f2f2f;
}

#WingMenuPlugin QFrame {
	background:			transparent;
}

#WingMenuPlugin ApplicationsView {
	color:				#e0e0e0;
	background:			#000000;

	border:				1px solid #2f2f2f;

	margin-right:			2px;
}

#WingMenuPlugin ApplicationsView::item {
	border:				4px solid transparent;

	padding-left:			6px;
	padding-right:			12px;
	padding-top:			0px;
	padding-bottom:			0px;

	color:				#e0e0e0;
	background:			transparent;

	margin:				1px;
}

#WingMenuPlugin ApplicationsView::item:selected {
	color:				#ffffff;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(hover.png) 4;
}

#WingMenuPlugin QToolButton, 
#WingMenuPlugin #CategoryButton {
	color:				#e0e0e0;
	background:			#000000;

	border:				4px solid transparent;
	border-image:			none;

	font:				normal;

	margin-right:			1px;
}

#WingMenuPlugin QToolButton:hover, 
#WingMenuPlugin #CategoryButton:hover, 
#WingMenuPlugin #CategoryButton:selected {
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(hover.png) 4;
}

#WingMenuPlugin QToolButton:pressed {
	color:				#e0e0e0;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(active.png) 4;
}

#WingMenuPlugin #CategoryButton:pressed {
	color:				#e0e0e0;
	background:			#16162f;

	border:				4px solid transparent;
	border-image:			url(active.png) 4;
}

#WingMenuPlugin QLineEdit {
	color:				#e0e0e0;
	background:			#000000;

	selection-color:		#ffffff;
	selection-background-color:	#16162f;

	border:				2px solid transparent;
	border-image:			url(entry.png) 2;

	margin:				2px;
	padding:			2px;
}

#WingMenuPlugin QScrollBar {
	color:				#e0e0e0;
	background:			#212121;

	border:				1px solid #4a4a4a;

	width:				16px;
}

#WingMenuPlugin QScrollBar::handle {
	color:				#e0e0e0;
	background:			#000000;

	background-image:		url(handle.png);
	background-repeat:		repeat;
	background-attachment:		fixed;
	background-position:		center;

	border:				2px solid transparent;
	border-image:			url(scrollbar.png) 2;

	margin:				-1px;
}

#WingMenuPlugin QScrollBar::add-line,
#WingMenuPlugin QScrollBar::sub-line,
#WingMenuPlugin QScrollBar::add-page,
#WingMenuPlugin QScrollBar::sub-page {
        color:                          transparent;
        background:                     transparent;

        border:                         none;
}

#KbIndicator > QLabel, #KbIndicator > QToolButton {padding: 2px;}

#KbIndicator #LayoutLabel {background: transparent;}

#KbIndicator #CapsLockLabel {color: #52501b; font: normal;}
#KbIndicator #CapsLockLabel:enabled {color: #e0e0e0; font: bold;}
#KbIndicator #CapsLockLabel:!enabled {color: #e0e0e0; font: normal;}
#KbIndicator #CapsLockLabel:disabled {color: #52501b; font: normal;}

#KbIndicator #NumLockLabel {color: #52501b; font: normal;}
#KbIndicator #NumLockLabel:enabled {color: #e0e0e0; font: bold;}
#KbIndicator #NumLockLabel:!enabled {color: #e0e0e0; font: normal;}
#KbIndicator #NumLockLabel:disabled {color: #52501b; font: normal;}

#KbIndicator #ScrollLockLabel {color: #52501b; font: normal;}
#KbIndicator #ScrollLockLabel:enabled {color: #e0e0e0; font: bold;}
#KbIndicator #ScrollLockLabel:!enabled {color: #e0e0e0; font: normal;}
#KbIndicator #ScrollLockLabel:disabled {color: #52501b; font: normal;}

/* Backlight and Volume */
#Backlight, #Volume {
        color:                          #e0e0e0;
        background:                     #000000;
}

#Backlight SliderDialog, VolumePopup {
        color:                          #e0e0e0;
        background:                     #000000;

        border:                         1px solid #2f2f2f;
}

#Backlight > SliderDialog > QSlider,
VolumePopup > QSlider {
        color:                          #e0e0e0;
        background:                     #000000;

        min-width:                      20px;
        max-width:                      20px;
        min-height:                     140px;
        max-height:                     140px;

	margin-top:			4px;
	margin-bottom:			4px;
}

#Backlight > SliderDialog > QSlider::groove:vertical, 
VolumePopup > QSlider::groove:vertical {
	color:				#e0e0e0;
	background:			#212121;

	border:				1px solid #4a4a4a;

	width:				16px;
}

#Backlight > SliderDialog > QSlider::sub-page:vertical,
VolumePopup > QSlider::sub-page:vertical {
}

#Backlight > SliderDialog > QSlider::handle:vertical,
VolumePopup > QSlider::handle:vertical {
	color:				#e0e0e0;
	background:			#000000;

	border:				2px solid transparent;
	border-image:			url(scrollbar.png) 2;

	background-image:		url(handle.png);
	background-repeat:		repeat;
	background-attachment:		fixed;
	background-position:		center;

	width:				8px;
	height:				32px;

	margin:				-1px;
}

#Backlight > SliderDialog > QToolButton,
VolumePopup > QPushButton {
	color:				#e0e0e0;
	background:			#000000;

	border:				4px solid transparent; 
	border-image:			url(button.png) 4;

        qproperty-iconSize:             16px;

	padding:			3px;

	margin:				2px;
}

#Backlight > SliderDialog > QToolButton:hover,
VolumePopup > QPushButton:hover {
	background:			#16162f;
	border-image:			url(button.png) 4;
}

#Backlight > SliderDialog > QToolButton:pressed,
VolumePopup > QPushButton:pressed {
	background:			#16162f;
	border-image:			url(active.png) 4;
}

QCalendarWidget QWidget {
	color:				#e0e0e0;
	background:			#ffffff;
}

QCalendarWidget QAbstractItemView {
        color:                          #e0e0e0;
        background-color:               #000000;

        alternate-background-color:     #000000;
}

QCalendarWidget QAbstractItemView:enabled {
        color:                          #e0e0e0;
        background:                     #000000;

        selection-color:                #ffffff;
        selection-background-color:     #16162f;        
}

QCalendarWidget QAbstractItemView:disabled {
        color:                          #52501b;
	background:			#ffffff;
}

QCalendarWidget QMenu {
        color:                          #e0e0e0;
        background:                     #000000;

        border:                         1px solid #2f2f2f;
}

QCalendarWidget QToolButton {
	color:				#e0e0e0;
        background:                     #000000;

	border:				4px solid transparent;

	margin:				2px;
}

QCalendarWidget QToolButton:hover {
	background:			#000000;
	border-image:			url(button.png) 4;
}

QCalendarWidget QToolButton:pressed {
	background:			#16162f;
	border-image:			url(active.png) 4;
}

QCalendarWidget QToolButton::menu-indicator { /* Remove the down arrow */
        image:                          none;
}

QCalendarWidget #qt_calendar_navigationbar {
        color:                          #e0e0e0;
        background:                     #000000;

        border:                         1px solid #2f2f2f;
}

QCalendarWidget #qt_calendar_prevmonth {
        qproperty-icon:                 url(calendar-popup/arrow-left.png);
        qproperty-iconSize:             16px;
}

QCalendarWidget #qt_calendar_nextmonth {
        qproperty-icon:                 url(calendar-popup/arrow-right.png);
        qproperty-iconSize:             16px;
}
